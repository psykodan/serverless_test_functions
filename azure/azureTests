
#node

#PrimeComp number calculation / Runtime test


seq 1 10 | xargs -n1 -P1  curl -s -w "\n" -X GET "https://nodefunctiondk.azurewebsites.net/api/nodePrimeComp">>nodePrimeComp/nodePrimeComp.txt

seq 1 1 | xargs -n1 -P1  curl -s -w "\n" -X GET "https://nodefunctiondk.azurewebsites.net/api/nodePrimeComp">>nodePrimeComp/nodePrimeComp-1.txt
seq 1 1000 | xargs -n1 -P1000  curl -s -w "\n" -X GET "https://nodefunctiondk.azurewebsites.net/api/nodePrimeComp">>nodePrimeComp/nodePrimeComp-1000.txt



